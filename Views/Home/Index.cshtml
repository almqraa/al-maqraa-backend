    <!-- to random team members-->
    <script>
        document.addEventListener("DOMContentLoaded", function () {
            const container = document.getElementById('team-members');
            const teamMembers = Array.from(container.getElementsByClassName('card-container'));

            // Shuffle function
            function shuffle(array) {
                for (let i = array.length - 1; i > 0; i--) {
                    const j = Math.floor(Math.random() * (i + 1));
                    [array[i], array[j]] = [array[j], array[i]];
                }
                return array;
            }

            // Shuffle team members
            const shuffledTeamMembers = shuffle(teamMembers);

            // Clear existing team members
            while (container.firstChild) {
                container.removeChild(container.firstChild);
            }

            // Append shuffled team members
            shuffledTeamMembers.forEach(member => container.appendChild(member));
        });
    </script>
